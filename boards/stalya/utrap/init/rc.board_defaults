#!/bin/sh
#
# Board specific defaults
#------------------------------------------------------------------------------

param set-default BAT1_V_DIV 38.25
param set-default BAT2_V_DIV 38.25

param set-default BAT1_A_PER_V 0
param set-default BAT2_A_PER_V 0

# Enable IMU thermal control
param set-default SENS_EN_THERMAL 1
param set-default -s SENS_TEMP_ID 3932170

set IOFW "/fs/microsd/etc/extras/stalya_io-v2_default.bin"

safety_button start
